text/html; /usr/bin/firefox %s; description=HTML Text; test=test -n "$DISPLAY";  nametemplate=%s.html; priority=5
text/xml; /usr/bin/firefox %s; description=XML Text; test=test -n "$DISPLAY";  nametemplate=%s.xml; priority=5
image/png;  /usr/bin/firefox %s; description=PNG Image; test=test -n "$DISPLAY";  nametemplate=%s.png; priority=2
image/jpeg;  /usr/bin/firefox %s; description=JPEG Image; test=test -n "$DISPLAY";  nametemplate=%s.jpeg; priority=2
image/gif;  /usr/bin/firefox %s; description=GIF Image; test=test -n "$DISPLAY";  nametemplate=%s.gif; priority=2
